citHeader("To cite package 'secrBVN' in publications use:")
year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date)
vers <- paste("R package version", meta$Version)
citEntry(entry="Manual",
title = "secrBVN: SECR with Bivariate-Normal Home Ranges",
author = personList(as.person("Murray Efford")),
year = year,
note = vers,
textVersion =
paste("Efford, M. G. (",
year,
"). secrBVN: SECR with Bivariate-Normal Home Ranges ",
vers, ".", sep=""))